{
  "name": "sulaiman-launch-apps",
  "version": "0.1.0",
  "description": "search and launch apps installed on your operating system",
  "main": "index.js",
  "private": true,
  "homepage": "https://gitlab.com/hpj/sulaiman-app-launch#readme",
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/hpj/sulaiman-app-launch.git"
  },
