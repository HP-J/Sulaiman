    "url": "https://gitlab.com/hpj/sulaiman-app-launch/issues"
  },
  "sulaiman": {
    "displayName": "Launch Apps",
    "permissions": [
      "process",
      "shell"
    ],
    "modules": [
      "fs-extra",
      "path",
      "os",
      "child_process"
    ],
    "platform": [
      "linux",
      "win32"
    ],
    "credits": [
      [
        "KELiON",
        "for his already established work on figuring this out",
        "https://www.github.com/KELiON/cerebro-basic-apps"
      ]
    ]
  },
  "author": "Herp Project <herpproject0@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "fs-extra": "^7.0.1"
  }
}
