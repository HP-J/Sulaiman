{
  "name": "sulaiman-app-launch",
  "version": "0.1.0",
  "description": "search and launch apps installed on your operating system",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/herpproject/sulaiman-app-launch.git"
  },
  "sulaiman": {
    "displayName": "Apps",
    "permissions": [
      "shell"
    ],
    "modules": [
      "fs",
      "path",
      "os",
      "child_process"
    ],
    "platform": [
      "linux",
      "win32"
    ],
    "credits": {
      "KELiON\nfor his already established work on figuring this out": "https://github.com/KELiON/cerebro-basic-apps"
    }
  },
  "author": "Herp Project",
