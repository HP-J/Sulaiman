{
  "name": "sulaiman",
  "version": "0.1.0-2",
  "description": "In no way, shape, or form is this the package you need to create sulaiman extensions",
  "main": "index.js",
  "private": true,
  "scripts": {
    "dev": "npx babel src --out-dir public --ignore node_modules --source-maps --copy-files --watch",
    "compile": "npx babel src --out-dir public --ignore node_modules --source-maps --copy-files",
    "clean": "rm -r ./public/",
    "test": "eslint ./src ./test && nyc mocha --require babel-core/register --require babel-polyfill"
  },
  "nyc": {
    "include": [
      "**/main.js",
      "**/card.js",
      "**/elements.js",
      "**/extension.js",
      "**/registry.js",
      "**/renderer.js"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/herpproject/sulaiman.git"
  },
  "author": "Herp Project",
  "license": "GPL-3.0",
  "dependencies": {
    "electron": "^3.0.0-beta.2",
    "pidtree": "^0.3.0",
    "vm2": "^3.6.2"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.1.0",
    "mocha": "^5.2.0",
    "nyc": "^12.0.2",
    "puppeteer": "^1.2.0",
    "puppeteer-to-istanbul": "^1.2.2",
    "request-promise": "^4.2.2"
  }
}
