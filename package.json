{
  "name": "sulaiman",
  "productName": "Sulaiman",
  "version": "0.1.1",
  "description": "A keyboard launcher that doesn't exists",
  "main": "index.js",
  "scripts": {
    "dev": "npx babel src --out-dir compiled --copy-files --source-maps --watch & npx pug src --out compiled --pretty --watch &",
    "compile": "npx babel src --out-dir compiled --copy-files --no-comments --minified && npx pug src --out compiled && rm compiled/*.pug"
  },
  "build": {
    "appId": "com.herpproject.sulaiman",
    "afterPack": "afterPack.js",
    "asar": false,
    "files": [
      "index.js",
      "build.json",
      "LICENSE",
      "README.md",
      "CHANGELOG.md",
      "tray-black.png",
      "tray-dark.png",
      "tray-light.png"
    ],
    "directories": {
      "output": "./public"
    },
    "linux": {
      "artifactName": "Sulaiman.${ext}",
      "executableName": "Sulaiman",
      "category": "Utility"
    },
    "nsis": {
      "artifactName": "Sulaiman-Setup.exe",
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "uninstallDisplayName": "Sulaiman",
      "shortcutName": "Sulaiman"
    },
    "electronDownload": {
      "cache": ".electron-cache"
    }
  },
  "homepage": "https://gitlab.com/herpproject/Sulaiman#readme",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com/herpproject/Sulaiman.git"
  },
  "bugs": {
    "url": "https://gitlab.com/herpproject/Sulaiman/issues"
  },
  "author": "Herp Project <herpproject0@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "auto-launch": "^5.0.5",
    "fs-extra": "^7.0.1",
    "inly": "^3.0.3",
    "npm": "^6.9.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.7",
    "string-similarity": "^3.0.0",
    "vm2": "^3.6.10"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "babel-eslint": "^10.0.1",
    "electron": "^4.0.8",
    "electron-builder": "^20.38.5",
    "eslint": "^5.15.1",
    "pug": "^2.0.3",
    "pug-cli": "^1.0.0-alpha6",
    "pug-lint": "^2.5.0",
    "pug-lint-config-clock": "^2.0.0"
  }
}
